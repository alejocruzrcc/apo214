<div class='div_ubicacionpre otrolugarasesinato' style='<%= estol %>' >
  <% ph = ''%>
  <% if (!f.object.ubicacionpre) %>
    <% ph = 'Buscar'%>
  <% end %>
  <% ac = { } %>
  <% ac['data-toogle']='tooltip' %>
  <% ac['title']='Empiece a escribir el municipio o departamento y cuando lo vea en el listado de autocompletados, elijalo.' %>
  Otro municipio o centro poblado:
  <%= f.input :otrolubicacionpre_texto, placeholder: ph,
    label: false,
    input_html: ac %>
  <%= f.input :otrolubicacionpre_id, as: :hidden %>
</div>
