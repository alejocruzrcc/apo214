<%= turbo_frame_tag dom_id(asisreconocimiento) do %>
  <%= asisreconocimiento.organizacion %>
  <%= asisreconocimiento.asistente.nombres if asisreconocimiento.asistente %>
  <%= asisreconocimiento.asistente.apellidos if asisreconocimiento.asistente %>
  <%= asisreconocimiento.asistente.sexo if asisreconocimiento.asistente %>
  <%= link_to 'Editar', [:edit, asisreconocimiento.lugarpreliminar, asisreconocimiento] %>
  <%= link_to 'Eliminar', lugarpreliminar_asisreconocimiento_path(@lugarpreliminar, asisreconocimiento), method: :delete %>
<% end %>
